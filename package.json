{
  "name": "stylus-linter",
  "version": "1.6.5",
  "description": "A linter for stylus, stylint customize",
  "author": "Ross Patton",
  "license": "GPL-3.0",
  "keywords": [
    "cli",
    "css",
    "hinter",
    "lint",
    "linter",
    "styl",
    "style",
    "stylint",
    "stylus",
    "stylus-hint",
    "stylus-lint"
  ],
  "main": "index.js",
  "files": [
    "bin/",
    "index.js",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RootCisco/stylus-lint"
  },
  "bugs": {
    "url": "https://github.com/RootCisco/stylus-lint/issues"
  },
  "bin": {
    "stylint": "./bin/stylint"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "eslint": "5.0.0",
    "istanbul": "0.4.5",
    "jsdoc": "3.5.5",
    "mocha": "5.2.0",
    "sinon": "6.0.1",
    "touch": "3.1.0"
  },
  "dependencies": {
    "async": "2.6.1",
    "chalk": "2.4.1",
    "chokidar": "2.0.4",
    "columnify": "1.5.4",
    "glob": "7.1.2",
    "lodash.defaults": "4.2.0",
    "path-is-absolute": "1.0.1",
    "stampit": "4.1.2",
    "strip-json-comments": "2.0.1",
    "user-home": "2.0.0",
    "yargs": "11.0.0"
  },
  "scripts": {
    "cover": "istanbul cover node_modules/.bin/_mocha -- -u exports -R spec test/test.js",
    "docs": "jsdoc src/ --recurse --destination docs/",
    "lint": "eslint .",
    "precover": "npm run lint",
    "pretest": "npm run lint",
    "start": "node --harmony index.js",
    "test": "mocha"
  }
}
